<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://struts.apache.org/dtds/struts-config_1_2.dtd">

<struts-config>
    <form-beans>
        <form-bean name="FacebookLoginForm" type="com.projectplus.login.FacebookLoginForm"/>
        <form-bean name="MemberActionForm" type="com.projectplus.member.MemberActionForm"/>
        <form-bean name="TeamActionForm" type="com.projectplus.team.TeamActionForm"/>
		<form-bean name="ProjectActionForm" type="com.projectplus.project.ProjectActionForm"/>
		<form-bean name="ArticleForm" type="com.profile.article.ArticleForm"/>
        <form-bean name="ProjectActionForm" type="com.projectplus.project.ProjectActionForm"/>
    </form-beans>

  <action-mappings>
  
  	<action name="FacebookLoginForm" path="/FacebookLoginAction" scope="request" type="com.projectplus.login.FacebookLoginAction">
  	</action>

  	<action name="MemberActionForm" path="/MemberAction" scope="request" type="com.projectplus.member.MemberAction">
  	</action>
  	
	<action name="TeamActionForm" path="/TeamAction" scope="request" type="com.projectplus.team.TeamAction">
	</action>
	
	<action name="ProjectActionForm" path="/ProjectAction" scope="request" type="com.projectplus.project.ProjectAction">
	</action>
	
	<action name="ArticleForm" path="/ArticleAction" scope="request" type="com.profile.article.ArticleAction">
	</action>
	
	<action name="ProjectActionForm" path="/ProjectAction" scope="request" type="com.projectplus.project.ProjectAction">
	</action>
	
  </action-mappings>


  <message-resources parameter="ApplicationResources" />
</struts-config>

